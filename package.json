{
    "name": "channel-service",
    "version": "0.2.2",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "env": "bun --print process.env",
        "prepare": "husky",
        "lint": "eslint .",
        "format": "prettier --write .",
        "clean": "rm -rf dist",
        "build": "bun run clean && bun run lint && bun run format && bun build.ts && echo 'Done'.",
        "publish": "bun run build && bun publish --access public",
        "publish:dry-run": "bun run build && bun publish --dry-run",
        "bump": "bunx bumpp"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@typescript-eslint/eslint-plugin": "^8.2.0",
        "@typescript-eslint/parser": "^8.2.0",
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^9.1.5",
        "prettier": "^3.3.3",
        "typescript-eslint": "^8.2.0",
        "zod": "^3.23.8"
    },
    "dependencies": {
        "@logtape/logtape": "^0.4.3",
        "@supabase/supabase-js": "^2.48.1"
    }
}
